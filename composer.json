{
    "name": "zerosdev/kirimwa-php-client",
    "description": "Client SDK Library for Kirimwa.cloud",
    "type": "library",
    "homepage": "https://kirimwa.cloud",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/zerosdev/kirimwa-php-client/issues",
        "source": "https://github.com/zerosdev/kirimwa-php-client"
    },
    "authors": [
        {
            "name": "Zeros Technology",
            "email": "admin@zeros.co.id"
        }
    ],
    "minimum-stability": "dev",
    "require": {
    	"php": ">=5.6",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.0 || ^7.0"
    },
    "autoload": {
    	"psr-4" : {
          "ZerosDev\\KirimWA\\": "src"
        }
    },
    "require-dev": {
    	"phpunit/phpunit": "^7"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ZerosDev\\KirimWA\\Laravel\\ServiceProvider"
            ],
            "aliases": {
                "KirimWA": "ZerosDev\\KirimWA\\Laravel\\Facade"
            }
        }
    }
}